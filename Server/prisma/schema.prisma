
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id  Int @id @default(autoincrement())
  firstname  String
  lastname  String
  email  String @unique
  password  String
}

model Subscriber {
  id    Int     @id @default(autoincrement())
  email String  @unique
}